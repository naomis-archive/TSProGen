{
  "name": "tsprogen",
  "version": "1.2.0",
  "description": "A CLI project generator for setting up TypeScript + Node projects.",
  "main": "./prod/bin.js",
  "bin": {
    "tsprogen": "./bin/tsprogen"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "copyfiles -f src/* -e src/*.ts prod",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "node ./prod/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan/TSProGen.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nhcarrigan/TSProGen/issues"
  },
  "homepage": "https://github.com/nhcarrigan/TSProGen#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.2",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "enquirer": "^2.3.6",
    "fs-extra": "^9.0.1",
    "listr2": "^3.2.2"
  }
}
