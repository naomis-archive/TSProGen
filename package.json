{
  "name": "ts-pro-gen",
  "version": "0.0.0",
  "description": "A CLI project generator for setting up TypeScript + Node projects.",
  "main": "./prod/bin.js",
  "bin": {
    "ts-pro-gen": "./bin/ts-pro-gen"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "copyfiles -f src/* -e src/*.ts prod",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "node ./prod/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan/TSProGen.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nhcarrigan/TSProGen/issues"
  },
  "homepage": "https://github.com/nhcarrigan/TSProGen#readme",
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "copyfiles": "^2.4.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  }
}
